
@{
    ViewBag.Title = "Modificar";
}

@using Practica04092017.Models
@model  Materia

    <h2>Modificar</h2>
    <form action="/Home/ModificarDatos" method="Post">
        <!-- NO MUESTRO EL CONTENI. ME GENERA EL INPUT -->
